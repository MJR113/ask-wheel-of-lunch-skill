{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "Dining recommendations when you're hungry.",
          "examplePhrases": [
            "Alexa, open wheel of lunch",
            "Alexa, tell wheel of lunch I'm hungry",
            "Alexa, ask wheel of lunch to give me a recommendation"
          ],
          "keywords": [
            "lunch",
            "food",
            "recommendations"
          ],          
          "name": "Wheel of Lunch",
          "description": "Wheel of Lunch provides dining recommendations for when you're too hungry to think."  
        }
      },
      "isAvailableWorldwide": true,
      "testingInstructions": "Thank you for testing this skill.",
      "category": "EDUCATION_AND_REFERENCE",
      "distributionCountries": []
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom"
        }
      }
    },
    "manifestVersion": "1.0",
    "permissions": [
      {
        "name": "alexa::devices:all:address:full:read"
      }
    ],
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "isExportCompliant": true,
      "containsAds": false,
      "isChildDirected": false,
      "usesPersonalInfo": false
    }    
  }
}
